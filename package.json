{
  "name": "gatsby-starter-tailwind",
  "description": "Site pour le collectif Fossile futur",
  "version": "5.0.0",
  "author": "Clément et Léa",
  "dependencies": {
    "gatsby": "2.32.13",
    "gatsby-plugin-anchor-links": "^1.2.1",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-offline": "3.10.2",
    "gatsby-plugin-postcss": "3.7.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-sharp": "^3.6.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-hook-inview": "^4.3.10",
    "react-responsive-carousel": "^3.2.18"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.2",
    "autoprefixer": "10.2.5",
    "babel-eslint": "10.1.0",
    "cssnano": "5.0.2",
    "eslint": "7.26.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-react": "7.28.0",
    "gatsby-plugin-eslint": "2.0.8",
    "postcss": "8.2.15",
    "prettier": "2.3.0",
    "tailwindcss": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
